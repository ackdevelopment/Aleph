// this is an external declaration of C's printf() function
extern proc printf(fmt: const char *) -> void

proc fizz_buzz(n: int) = {
    if(n > 0){
        fizz_buzz(n - 1)
    }
    if(n < 0){
        return;
    }
    if(n % 5){
        printf("Fizz")
    }
    if(n % 3){
        printf("Buzz")
    }
}

proc main = {
    fizz_buzz(100)
    0
}
